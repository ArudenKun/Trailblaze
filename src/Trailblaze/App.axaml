<Application
    x:Class="Trailblaze.App"
    xmlns="https://github.com/avaloniaui"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:suki="https://github.com/kikipoulet/SukiUI"
    RequestedThemeVariant="Default">

    <Application.Resources>
        <RawEnumToCollectionConverter x:Key="RawEnumToCollectionConverter" />
        <FromStringToEnumConverter x:Key="FromStringToEnumConverter" />
        <EnumToCollectionConverter x:Key="EnumToCollectionConverter" />
    </Application.Resources>

    <Application.Styles>
        <suki:SukiTheme ThemeColor="Blue" />
        <TrailblazeTheme />
    </Application.Styles>
</Application>